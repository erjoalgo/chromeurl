{
    "name": "Chrome Current Url",
    "description" : "Make chrome's currently active url available to other native processes",
    "version": "2025.10.31.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "nativeMessaging",
        "notifications"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "512": "link-512.png",
        "128": "link-128.png"
    }
}
